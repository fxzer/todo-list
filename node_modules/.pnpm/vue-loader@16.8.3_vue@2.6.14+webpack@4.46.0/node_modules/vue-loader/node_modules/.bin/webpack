#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+webpack@4.46.0/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+webpack@4.46.0/node_modules/webpack/bin/webpack.js" "$@"
fi
